@using Configastic.Components.Features.Scanner.Pages.ViewModels

<MudListItem Clickable="true"
             Dense="true"
             T="object">
    @foreach (var inputVM in InputViewModels)
    {
        <InputListItem Instance=inputVM/>
    }
</MudListItem>

@code {
    [Parameter]
    public List<InputViewModel> InputViewModels { get; set; } = [];
}
